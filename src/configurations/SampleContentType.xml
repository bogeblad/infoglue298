	<xs:complexType name="Validation">
		<xs:annotation>
			<xs:appinfo>
				<form-validation>
					<global>
						<validator name="required" classname="org.infoglue.cms.util.validators.CommonsValidator" method="validateRequired" methodParams="java.lang.Object,org.apache.commons.validator.Field" msg="300"/>
						<validator name="requiredif" classname="org.infoglue.cms.util.validators.CommonsValidator" method="validateRequiredIf" methodParams="java.lang.Object,org.apache.commons.validator.Field,org.apache.commons.validator.Validator" msg="315"/>
						<validator name="matchRegexp" classname="org.infoglue.cms.util.validators.CommonsValidator" method="validateRegexp" methodParams="java.lang.Object,org.apache.commons.validator.Field" msg="300"/>
					</global>
					<formset>
						<form name="requiredForm">
							<field property="Title" depends="required">
								<msg name="required" key="300"/>
							</field>
							<field property="Title" depends="matchRegexp">
								<var>
									<var-name>regexp</var-name>
									<var-value>[a-zA-Z]{0,20}</var-value>
								</var>
								<msg name="matchRegexp" key="301"/>
							</field>
							<field property="NavigationTitle" depends="requiredif">
								<var>
									<var-name>dependent</var-name>
									<var-value>Title</var-value>
								</var>
								<msg name="required" key="300"/>
							</field>
							<field property="Leadin" depends="required">
								<msg name="required" key="300"/>
							</field>
							<field property="FullText" depends="required">
								<msg name="required" key="300"/>
							</field>
						</form>
					</formset>
				</form-validation>
			</xs:appinfo>
		</xs:annotation>
